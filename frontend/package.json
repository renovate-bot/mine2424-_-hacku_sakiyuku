{
    "name": "sakiyuku_frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "run-p dev:*",
        "dev:next": "next dev",
        "dev:path": "pathpida --enableStatic --watch",
        "build": "pathpida --enableStatic && next build",
        "analyze": "ANALYZE=true next build",
        "start": "next start",
        "typecheck": "tsc --noEmit",
        "export": "next build && next export",
        "lint": "next lint",
        "jest": "jest",
        "jest:watch": "jest --watch",
        "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
        "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
        "format": "prettier --write \"**/*.{ts,tsx}\" && eslint --fix \"**/*.{ts,tsx}\"",
        "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
        "storybook": "start-storybook -p 7001",
        "storybook:build": "build-storybook",
        "prepare": "cd .. && husky install ./frontend/.husky"
    },
    "dependencies": {
        "@emotion/react": "^11.10.6",
        "@emotion/server": "^11.10.0",
        "@mantine/carousel": "6.0.2",
        "@mantine/core": "6.0.2",
        "@mantine/dates": "^6.0.2",
        "@mantine/ds": "^6.0.2",
        "@mantine/form": "^6.0.2",
        "@mantine/hooks": "6.0.2",
        "@mantine/next": "6.0.2",
        "@mantine/notifications": "6.0.2",
        "@mantine/prism": "6.0.2",
        "@next/bundle-analyzer": "^13.2.4",
        "@tabler/icons-react": "^2.10.0",
        "axios": "^1.3.4",
        "chart.js": "^4.2.1",
        "cookies-next": "^2.1.1",
        "dayjs": "^1.11.7",
        "embla-carousel-react": "^7.1.0",
        "firebase": "^9.18.0",
        "next": "13.2.4",
        "react": "18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "18.2.0",
        "react-fade-in": "^2.0.1",
        "recoil": "^0.7.7"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@next/eslint-plugin-next": "^13.2.4",
        "@storybook/react": "^6.5.16",
        "@testing-library/dom": "^9.0.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.3",
        "@types/react": "18.0.28",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.2",
        "eslint": "^8.36.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-mantine": "2.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.11",
        "eslint-plugin-testing-library": "^5.10.2",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.0",
        "next-intercept-stdout": "^1.0.1",
        "npm-run-all": "^4.1.5",
        "pathpida": "^0.20.1",
        "postcss": "^8.4.21",
        "prettier": "^2.8.4",
        "storybook-addon-turbo-build": "^1.1.0",
        "storybook-dark-mode": "^2.1.1",
        "ts-jest": "^29.0.5",
        "typescript": "5.6.3"
    },
    "lint-staged": {
        "./src/*.{ts,tsx}": [
            "pnpm format",
            "pnpm typecheck"
        ]
    }
}
